{
  "name": "hebo-validation",
  "description": "validation functions for hebo-js",
  "version": "5.0.0",
  "author": "Steve Caldwell <scaldwell@gmail.com>",
  "ava": {
    "failFast": true,
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/stevecaldwell77/hebo-validation/issues",
    "email": "scaldwell@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Steve Caldwell <scaldwell@gmail.com>"
  ],
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "ava": "^2.4.0",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-archereducation": "^4.3.1",
    "fixpack": "^2.3.1",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prettier-config-archereducation": "^1.0.0",
    "shortid": "^2.2.15"
  },
  "engines": {
    "node": ">=8.16.1"
  },
  "homepage": "https://github.com/stevecaldwell77/hebo-validation",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "cqrs",
    "es",
    "hebo-js"
  ],
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "*.js": [
        "yarn lint --fix",
        "git add"
      ],
      "package.json": [
        "fixpack",
        "git add"
      ]
    }
  },
  "main": "index.js",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevecaldwell77/hebo-validation"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint .",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "npm run lint && npm run ava",
    "test-coverage": "npm run lint && npm run nyc"
  }
}
